{
  "expo": {
    "name": "Get Your Life Right",
    "slug": "gylr",
    "version": "1.0.0",
    "scheme": "gylr",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#2C2137"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.gylr.app",
      "buildNumber": "1",
      "infoPlist": {
        "NSCalendarsUsageDescription": "GYLR uses your calendar to show how you spend your time and provide personalized insights.",
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2C2137"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.gylr.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-web-browser",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#2C2137",
          "image": "./assets/splash-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "b26f3e5a-ae75-4f56-84e8-a56ab9f239bb"
      }
    },
    "owner": "dravod"
  }
}
